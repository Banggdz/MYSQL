USE NORTHWIND;
SELECT Count(customerID),country
from customers
group by country

USE NORTHWIND;
SELECT MAX(price),SupplierID
from products
group by SupplierID

USE NORTHWIND;
SELECT COUNT(customerID),Country
from customers
group by Country
Having count(customerID)>=5

